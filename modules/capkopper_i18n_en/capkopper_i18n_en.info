name = capKopper Multilingual - English
description = Handles English language integration.
core = 7.x
package = capKopper
version = 7.x-1.0-rc2
project = capkopper_i18n_en
dependencies[] = capkopper_i18n
dependencies[] = ctools
dependencies[] = defaultconfig
dependencies[] = fe_date
dependencies[] = features
dependencies[] = i18n_variable
dependencies[] = locale
dependencies[] = panopoly_core
dependencies[] = strongarm
dependencies[] = system
dependencies[] = variable_realm
features[ctools][] = strongarm:strongarm:1
features[defaultconfig][] = strongarm:pathauto_node_panopoly_page_en_pattern
features[defaultconfig][] = strongarm:pathauto_taxonomy_term_panopoly_categories_en_pattern
features[defaultconfig][] = strongarm:pathauto_user_user_en_pattern
features[fe_date_locale_date_format][] = long::en
features[fe_date_locale_date_format][] = medium::en
features[fe_date_locale_date_format][] = panopoly_day::en
features[fe_date_locale_date_format][] = panopoly_time::en
features[fe_date_locale_date_format][] = short::en
features[features_api][] = api:2
features[language][] = en
features[variable_realm][] = language:en
features_exclude[variable][date_format_long] = date_format_long
features_exclude[variable][date_format_medium] = date_format_medium
features_exclude[variable][date_format_short] = date_format_short
